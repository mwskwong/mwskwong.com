{
  "name": "resume",
  "version": "4.0.0",
  "private": true,
  "scripts": {
    "prebuild": "npm run prisma:migrate:deploy",
    "build": "next build",
    "dev": "next dev",
    "postinstall": "npm run prisma:generate",
    "lint": "next lint --no-cache",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "prisma:generate": "prisma generate",
    "prisma:migrate:deploy": "dotenv -e .env.local -- prisma migrate deploy",
    "prisma:migrate:dev": "dotenv -e .env.local -- prisma migrate dev",
    "start": "next start",
    "stylelint": "stylelint **/*.css",
    "upgrade": "npm update && npm run postinstall && tsc --fix && npm run lint -- --fix && npm run stylelint -- --fix && npm run prettier:write"
  },
  "dependencies": {
    "@prisma/client": "^6.1.0",
    "@radix-ui/themes": "^3.2.0-rc.3",
    "@tabler/icons-react": "^3.26.0",
    "@vercel/analytics": "^1.4.1",
    "@vercel/speed-insights": "^1.1.0",
    "clsx": "^2.1.1",
    "contentful": "^11.3.4",
    "dedent": "^1.5.3",
    "feed": "^4.2.2",
    "next": "canary",
    "radix-themes-tw": "^0.2.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "server-only": "^0.0.1",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "canary",
    "@next/eslint-plugin-next": "canary",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@vercel/style-guide": "^6.0.0",
    "autoprefixer": "^10.4.20",
    "babel-plugin-react-compiler": "latest",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.57.1",
    "eslint-plugin-react-compiler": "latest",
    "postcss": "^8.4.49",
    "postcss-import": "^16.1.0",
    "prettier": "^3.4.2",
    "prettier-plugin-prisma": "^5.0.0",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "prisma": "^6.1.0",
    "schema-dts": "^1.1.2",
    "stylelint": "^16.12.0",
    "stylelint-config-standard": "^36.0.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2"
  }
}
