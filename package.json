{
  "name": "resume",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "prebuild": "npm run prisma:migrate:deploy",
    "build": "next build",
    "dev": "next dev",
    "email:dev": "dotenv -e .env -e .env.local -- email dev --dir src/components/emails --port 3001",
    "postinstall": "patch-package && npm run prisma:generate",
    "lint": "next lint",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "prisma:generate": "prisma generate",
    "prisma:migrate:deploy": "dotenv -e .env.local -- prisma migrate deploy",
    "prisma:migrate:dev": "dotenv -e .env.local -- prisma migrate dev",
    "start": "next start",
    "upgrade": "npm update && npm run postinstall && npm run prettier:write && tsc && npm run lint -- --fix"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@hookform/resolvers": "^3.6.0",
    "@icons-pack/react-simple-icons": "^9.6.0",
    "@mui/base": "next",
    "@mui/joy": "next",
    "@mui/material-nextjs": "next",
    "@mui/styled-engine": "next",
    "@prisma/client": "^5.16.1",
    "@react-email/body": "^0.0.8",
    "@react-email/container": "^0.0.12",
    "@react-email/hr": "^0.0.8",
    "@react-email/html": "^0.0.8",
    "@react-email/img": "^0.0.8",
    "@react-email/link": "^0.0.8",
    "@react-email/text": "^0.0.8",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@vercel/analytics": "^1.3.1",
    "@vercel/speed-insights": "^1.0.12",
    "contentful": "^10.12.4",
    "dedent": "^1.5.3",
    "feed": "^4.2.2",
    "geist": "^1.3.0",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.400.0",
    "merge-sx": "^2.0.2",
    "next": "canary",
    "next-mdx-remote": "^5.0.0",
    "react": "rc",
    "react-dom": "rc",
    "react-email": "^2.1.5",
    "react-hook-form": "^7.52.1",
    "rehype-pretty-code": "^0.13.2",
    "rehype-slug": "^6.0.0",
    "resend": "^3.4.0",
    "schema-dts": "^1.1.2",
    "server-only": "^0.0.1",
    "shiki": "^1.10.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "canary",
    "@next/eslint-plugin-next": "canary",
    "@svgr/webpack": "^8.1.0",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.14.9",
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc",
    "@vercel/style-guide": "^6.0.0",
    "babel-plugin-react-compiler": "latest",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react-compiler": "latest",
    "patch-package": "^8.0.0",
    "prettier": "^3.3.2",
    "prettier-plugin-prisma": "^5.0.0",
    "prisma": "^5.16.1",
    "type-fest": "^4.21.0",
    "typescript": "^5.5.3"
  },
  "overrides": {
    "@mui/system": "next",
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc"
  }
