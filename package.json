    "@total-typescript/ts-reset": "^0.6.1",
    "@types/mdx": "^2.0.13",
    "@types/node": "^22.10.3",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@vercel/style-guide": "^6.0.0",
    "autoprefixer": "^10.4.20",
    "babel-plugin-react-compiler": "latest",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.57.1",
    "eslint-plugin-react-compiler": "latest",
    "patch-package": "^8.0.0",
    "postcss": "^8.4.49",
    "postcss-custom-media": "^11.0.5",
