{
  "name": "resume",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "prebuild": "npm run prisma:migrate:deploy",
    "build": "next build",
    "dev": "next dev",
    "email:dev": "dotenv -e .env -e .env.local -- email dev --dir src/components/emails --port 3001",
    "postinstall": "patch-package && npm run prisma:generate",
    "lint": "next lint",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "prisma:generate": "prisma generate",
    "prisma:migrate:deploy": "dotenv -e .env.local -- prisma migrate deploy",
    "prisma:migrate:dev": "dotenv -e .env.local -- prisma migrate dev",
    "start": "next start",
    "upgrade": "npm update && npm run postinstall && npm run prettier:write && tsc && npm run lint -- --fix"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@hookform/resolvers": "^3.3.4",
    "@icons-pack/react-simple-icons": "^9.4.1",
    "@mui/base": "next",
    "@mui/joy": "5.0.0-beta.36",
    "@mui/material-nextjs": "next",
    "@mui/styled-engine": "next",
    "@prisma/client": "^5.13.0",
    "@react-email/body": "^0.0.7",
    "@react-email/container": "^0.0.11",
    "@react-email/hr": "^0.0.7",
    "@react-email/html": "^0.0.7",
    "@react-email/img": "^0.0.7",
    "@react-email/link": "^0.0.7",
    "@react-email/text": "^0.0.7",
    "@t3-oss/env-core": "^0.10.1",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@vercel/analytics": "^1.2.2",
    "@vercel/speed-insights": "^1.0.10",
    "contentful": "^10.8.10",
    "dedent": "^1.5.3",
    "feed": "^4.2.2",
    "geist": "^1.3.0",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.373.0",
    "merge-sx": "^2.0.0",
    "next": "canary",
    "next-mdx-remote": "^4.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-email": "^2.1.1",
    "react-hook-form": "^7.51.3",
    "rehype-pretty-code": "^0.13.1",
    "rehype-slug": "^6.0.0",
    "resend": "^3.2.0",
    "schema-dts": "^1.1.2",
    "server-only": "^0.0.1",
    "shiki": "^1.3.0",
    "zod": "^3.23.4"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "canary",
    "@next/eslint-plugin-next": "canary",
    "@svgr/webpack": "^8.1.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@vercel/style-guide": "^6.0.0",
    "dotenv-cli": "^7.4.1",
    "eslint": "^8.57.0",
    "patch-package": "^8.0.0",
    "prettier": "^3.2.5",
    "prettier-plugin-prisma": "^5.0.0",
    "prisma": "^5.13.0",
    "type-fest": "^4.17.0",
    "typescript": "^5.4.5"
  }
}
