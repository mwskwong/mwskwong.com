    "@vercel/speed-insights": "^1.1.0",
    "clsx": "^2.1.1",
    "contentful": "^11.4.0",
    "dedent": "^1.5.3",
    "feed": "^4.2.2",
    "next": "^15.1.3",
    "next-mdx-remote": "^5.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rehype-pretty-code": "^0.14.0",
    "rehype-slug": "^6.0.0",
    "remark-github-blockquote-alert": "^1.3.0",
    "server-only": "^0.0.1",
