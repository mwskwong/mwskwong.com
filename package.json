{
  "name": "resume",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "prebuild": "npm run prisma:migrate:deploy",
    "build": "next build",
    "dev": "next dev",
    "email:dev": "dotenv -e .env -e .env.local -- email dev --dir src/components/emails --port 3001",
    "postinstall": "patch-package && npm run prisma:generate",
    "lint": "next lint --no-cache",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "prisma:generate": "prisma generate",
    "prisma:migrate:deploy": "dotenv -e .env.local -- prisma migrate deploy",
    "prisma:migrate:dev": "dotenv -e .env.local -- prisma migrate dev",
    "start": "next start",
    "upgrade": "npm update && npm run postinstall && npm run prettier:write && tsc && npm run lint -- --fix"
  },
  "dependencies": {
    "@emotion/cache": "^11.13.5",
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@hookform/resolvers": "^3.9.1",
    "@icons-pack/react-simple-icons": "^10.1.0",
    "@mui/base": "next",
    "@mui/joy": "next",
    "@mui/material-nextjs": "^6.1.8",
    "@mui/styled-engine": "^6.1.8",
    "@prisma/client": "^5.22.0",
    "@react-email/components": "^0.0.28",
    "@vercel/analytics": "^1.4.1",
    "@vercel/speed-insights": "^1.1.0",
    "contentful": "^11.2.3",
    "dedent": "^1.5.3",
    "feed": "^4.2.2",
    "geist": "^1.3.1",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.460.0",
    "merge-sx": "^3.2.4",
    "next": "canary",
    "next-mdx-remote": "^5.0.0",
    "react": "rc",
    "react-dom": "rc",
    "react-email": "^3.0.2",
    "react-hook-form": "^7.53.2",
    "rehype-pretty-code": "^0.14.0",
    "rehype-slug": "^6.0.0",
    "resend": "^4.0.1",
    "server-only": "^0.0.1",
    "shiki": "^1.23.1",
    "valibot": "beta"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "canary",
    "@next/eslint-plugin-next": "canary",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.17.6",
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc",
    "@vercel/style-guide": "^6.0.0",
    "babel-plugin-react-compiler": "latest",
    "dotenv-cli": "^7.4.4",
    "eslint": "^8.57.1",
    "eslint-plugin-react-compiler": "latest",
    "patch-package": "^8.0.0",
    "prettier": "^3.3.3",
    "prettier-plugin-prisma": "^5.0.0",
    "prisma": "^5.22.0",
    "schema-dts": "^1.1.2",
    "type-fest": "^4.27.0",
    "typescript": "^5.7.2"
  },
  "overrides": {
    "@mui/utils": "^6.1.8",
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc"
  }
}
