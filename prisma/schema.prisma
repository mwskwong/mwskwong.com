generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model BlogMetadata {
  id   String @id
  view Int    @default(0)
  likes Like[]
}

model Like {
  visitorId String
  blogId String
  blogMetadata BlogMetadata  @relation(fields: [blogId], references: [id], onDelete: Cascade, onUpdate: Cascade) 
  @@id([visitorId, blogId])
}
